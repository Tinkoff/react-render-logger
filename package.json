{
  "name": "react-render-logger",
  "version": "1.0.0",
  "description": "Decorator for log react component updates",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npm run clean && babel src --out-dir lib --ignore *.test.js",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "prepublish": "npm run lint && npm run test && npm run build",
    "test": "mocha --compilers js:babel-register src/**/*.test.js",
    "test:watch": "npm run test -- --watch src/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TinkoffCreditSystems/react-render-logger.git"
  },
  "keywords": [
    "react",
    "logger",
    "render",
    "performance"
  ],
  "author": "Andrew Laiff \"q.laiff@gmail.com\"",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TinkoffCreditSystems/react-render-logger/issues"
  },
  "homepage": "https://github.com/TinkoffCreditSystems/react-render-logger#readme",
  "dependencies": {
    "deep-diff": "^0.3.4",
    "performance-now": "^0.2.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb-base": "^4.0.0",
    "eslint-plugin-import": "^1.10.2",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.3",
    "sinon": "^1.17.4"
  }
}
